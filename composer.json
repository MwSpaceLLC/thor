{
  "name": "mwspace/thor",
  "type": "project",
  "description": "Make easy develop addon for WHMCS marketplace ðŸ˜±",
  "keywords": [
    "whmcs",
    "whmcs addon"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.1.3",
    "nikic/fast-route": "^2.0@dev",
    "phpmailer/phpmailer": "*",
    "symfony/console": "^4.3@dev",
    "symfony/debug": "^4.3@dev",
    "symfony/var-dumper": "^4.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Core\\": "core/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-root-package-install": [
      "@php -r \"rename(__DIR__.'/whmcs_addon.php',__DIR__.'/'.basename(__DIR__).'.php');\""
    ]
  }
}
